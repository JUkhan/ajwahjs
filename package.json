{
  "name": "ajwahjs",
  "version": "1.3.9",
  "description": "Framework agnostic reactive state management tools",
  "private": false,
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsdelivr": "dist/index.umd.js",
  "jsnext:main": "dist/index.es.js",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "build": "tsc && vite build",
    "prepack": "npm run build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.1",
    "ajwah-test": "^1.1.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.2",
    "vitest": "^0.34.1"
  },
  "peerDependencies": {
    "rxjs": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JUkhan/ajwahjs.git"
  },
  "keywords": [
    "angular",
    "react",
    "vue",
    "redux",
    "reactive",
    "rxjs",
    "unidirectional"
  ],
  "author": "Jasim Khan<jasim.uddin.khan@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JUkhan/ajwahjs/issues"
  },
  "homepage": "https://github.com/JUkhan/ajwahjs#readme"
}
